<fieldset>
  <%= bootstrap_form_for @session, label_errors: true, class: "form-horizontal" do |f| %>
    <%= yield f %>
    <%#= f.error_notification %>

    <div class="form-inputs">
      <%= f.text_field :name, class: "form-control", placeholder: "Session Name" %>
    </div>

    <div class="form-group"></div>
    <div class="form-inputs">
      <div id="date_range">
        <%= f.text_field :start_date, class: "form-control", :data => {'date-format' => 'YYYY/MM/DD'}, :placeholder => "Start Date" %>
      </div>
    </div>
    <div class="form-group"></div>
    <div class="form-inputs">
      <%= f.text_field :end_date, class: "form-control", :data => {'date-format' => 'YYYY/MM/DD'}, :placeholder => "End Date" %>
    </div>

    <div class="form-group"></div>

    <div class="form-actions">
      <%= f.submit "Submit", class: "btn btn-primary" %>
      <%= link_to 'Cancel', sessions_path, class: "btn btn-default", style: "color:#8B7C7C;" %>
    </div>
    
    <div class="form-group"></div>
  <% end %>
</fieldset>
