<%= simple_form_for(@batch) do |f| %>
  <%= f.error_notification %>

 <div class="form-inputs">
    <%= f.association :course %>
  </div>

 <div class="form-inputs">
    <%= f.input :batch_name %>
  </div>

<div class="form-inputs">
    <%= f.input :start_date %>
  </div>

<div class="form-inputs">
    <%= f.input :end_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
