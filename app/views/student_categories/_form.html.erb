
<%= simple_form_for(@student_category) do |f| %>
  <%= f.error_notification %>
  <%= yield f %>
  <div class="row">
    <div class="col-sm-5">
      
      <div class="form-inputs">
        <%= f.input :name, input_html: {class: "form-control"}, placeholder: "Category Name" %>
      </div>
      <div class="form-group"></div>

      <div class="form-actions">
        <%= f.button :submit, class: "btn btn-primary" %>
      </div>
      
      <div class="form-group"></div>
    </div>
  </div>
<% end %>


